{
  "name": "@cosmicmind/materialjs",
  "version": "0.0.1-rc-021323-5",
  "description": "A Material Design framework in TypeScript.",
  "keywords": [],
  "author": {
    "name": "Daniel Jonathan",
    "email": "daniel@cosmicverse.org"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmicmind/materialjs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/lib.es.js",
  "module": "dist/lib.es.js",
  "types": "dist/lib/vue/index.d.ts",
  "files": [
    "dist",
    "typings",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "build": "run-s build:lib",
    "build:lib": "vite build --config vite.config.lib.ts --mode production",
    "build:app": "vite build --config vite.config.app.ts --mode production",
    "build:server": "vite build --config vite.config.server.ts --mode production",
    "watch": "run-p watch:app watch:server",
    "watch:lib": "vite build --watch --config vite.config.lib.ts --mode development",
    "watch:app": "vite build --watch --config vite.config.app.ts --mode development",
    "watch:server": "vite build --watch --config vite.config.server.ts --mode development",
    "nodemon": "nodemon dist/server.js",
    "dev": "run-s dev:build:app dev:build:server; run-p watch nodemon",
    "dev:build": "run-s dev:build:lib",
    "dev:build:lib": "vite build --config vite.config.lib.ts --mode development",
    "dev:build:app": "vite build --config vite.config.app.ts --mode development",
    "dev:build:server": "vite build --config vite.config.server.ts --mode development",
    "start": "node dist/server.js",
    "release": "run-s build:app build:server start",
    "test": "run-s test:unit",
    "test:unit": "vitest run --config vite.config.test.ts --mode test",
    "test:watch": "vitest --config vite.config.test.ts --mode test",
    "test:coverage": "vitest run --coverage --config vite.config.test.ts --mode benchmark",
    "test:build": "run-s test:build:app test:build:server start",
    "test:build:app": "vite build --config vite.config.app.ts --mode test",
    "test:build:server": "vite build --config vite.config.server.ts --mode test",
    "docker:dev": "docker build --build-arg npm_token=${npm_token} --squash --target dev -t cosmicmind/materialjs:latest .",
    "docker:build": "docker build --build-arg npm_token=${npm_token} --squash --target release -t cosmicmind/materialjs:latest ."
  },
  "dependencies": {
    "@cosmicmind/foundationjs": "^0.0.1-rc-021323-1",
    "@koa/router": "^12.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^12.0.0",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^18.7.15",
    "@vitejs/plugin-vue": "^3.1.0",
    "jsdom": "^20.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-static": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.21",
    "sass": "^1.55.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-static-copy": "^0.13.0",
    "vitest": "^0.23.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitest/coverage-c8": "^0.23.4",
    "@vue/compiler-sfc": "^3.2.38",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "eslint-plugin-yml": "^1.1.0",
    "nodemon": "^2.0.20"
  }
}
